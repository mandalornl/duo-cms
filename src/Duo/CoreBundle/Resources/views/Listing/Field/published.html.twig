{%- set published = entity is published -%}
{%- if published is constant('Duo\\CoreBundle\\Entity\\Property\\PublishInterface::ALL') -%}
	{{- 'duo.admin.type.bool.yes' | trans -}}
{%- elseif published is constant('Duo\\CoreBundle\\Entity\\Property\\PublishInterface::PARTIAL') -%}
	{{- 'duo.core.type.publish.partially' | trans -}}
{%- else -%}
	{{- 'duo.admin.type.bool.no' | trans -}}
{%- endif -%}