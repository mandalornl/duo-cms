<div id="modal_confirm" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" aria-labelledby="modal_confirm_label">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="modal_confirm_label">...</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="{{ 'duo.admin.modal.global.close' | trans }}">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<p>...</p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">
					{{- 'duo.admin.modal.global.cancel' | trans -}}
				</button>
				<button type="button" class="btn btn-primary">
					{{- 'duo.admin.modal.global.ok' | trans -}}
				</button>
			</div>
		</div>
	</div>
</div>