{% extends '@DuoAdmin/Listing/create.html.twig' %}

{% block modal %}
	{% if entity is revisionable %}
		{% include '@DuoCore/Listing/Modal/revisions.html.twig' %}
	{% endif %}
	{% if entity is draftable %}
		{% include '@DuoCore/Listing/Modal/drafts.html.twig' %}
		{% include '@DuoCore/Listing/Modal/new_draft.html.twig' %}
	{% endif %}
	{{ parent() }}
{% endblock %}